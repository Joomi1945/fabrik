			class="readonly"
			label="COM_FABRIK_FIELD_FILENAME_LABEL"
			description="COM_FABRIK_FIELD_FILENAME_DESC"  />
			
		<field name="remote_site" 
			type="text"
			size="40"
			readonly="true" 
			class="readonly"
			label="COM_FABRIK_FIELD_REMOTE_SITE_LABEL"
			description="COM_FABRIK_FIELD_REMOTE_SITE_DESC"  />
		
		<field name="remote_user"
			type="test"
			size="40"
			readonly="true" 
			class="readonly"
			label="COM_FABRIK_FIELD_REMOTE_USER_LABEL" 
			description="COM_FABRIK_FIELD_REMOTE_USER_DESC" />
		
	
	</fieldset>
	</fields>
