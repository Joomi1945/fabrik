	<fieldset name="details">
	
		<field name="id" 
			type="text" 
			default="0" 
			required="true"
			readonly="true" 
			class="readonly"
			label="JGLOBAL_FIELD_ID_LABEL" 	
			description="JGLOBAL_FIELD_ID_DESC" />
			
		<field name="filename" 
			type="filecontent"
			size="40"
			path="logs/dbchangeset/"
